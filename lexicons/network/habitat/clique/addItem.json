{
    "lexicon": 1,
    "id": "network.habitat.clique.addItem",
    "defs": {
        "main": {
            "type": "procedure",
            "description": "Send an item to a specified habitat clique.",
            "permission": "authenticated",
            "input": {
                "encoding": "application/json",
                "schema": {
                    "type": "object",
                    "required": [
                        "item",
                        "cliqueID"
                    ],
                    "properties": {
                        "item": {
                            "type": "string",
                            "description": "The URI for the item to send to the clique, formatted as a habitat-uri."
                        },
                        "cliqueID": {
                            "type": "string",
                            "description": "The ID of the clique to send the item to, formatted as a habitat-uri."
                        }
                    }
                }
            }
        }
    }
}