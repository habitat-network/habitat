{
  "extends": "../../tsconfig.lib.json",
  "include": ["**/*.ts"],
  "compilerOptions": {
    "lib": ["ES2022", "DOM"],
    "verbatimModuleSyntax": false,

    /* Relaxed checking for generated code */
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false
  }
}
